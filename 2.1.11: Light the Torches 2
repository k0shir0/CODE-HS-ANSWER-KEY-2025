
for (let i = 0; i < 6; i++) {
    if(colorIs(Color.green)){
        paint(Color.purple)
    }
    if(colorIs(Color.gray)){
        paint(Color.yellow)
    }
    move();
}
turnLeft();
for(let i = 0; i < 4; i++){
    if(colorIs(Color.green)){
        paint(Color.purple)
    }
    if(colorIs(Color.gray)){
        paint(Color.yellow)
    }
    move(); 
}
turnLeft();
for(let i = 0; i < 6; i++){
    if(colorIs(Color.green)){
        paint(Color.purple)
    }
    if(colorIs(Color.gray)){
        paint(Color.yellow)
    }
    move();  
}
turnRight();
for(let i = 0; i < 4; i++){
    if(colorIs(Color.green)){
        paint(Color.purple)
    }
    if(colorIs(Color.gray)){
        paint(Color.yellow)
    }
    move();      
}
turnRight();
for(let i = 0; i < 9; i++){
    if(colorIs(Color.green)){
        paint(Color.purple)
    }
    if(colorIs(Color.gray)){
        paint(Color.yellow)
    }   
    move();
}
